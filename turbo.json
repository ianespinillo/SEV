{
    "$schema": "https://turbo.build/schema.json",
    "tasks":{
        "dev": {
            "cache": false,
            "dependsOn": ["^db:generate"]
        },
        "db:push": {
            "outputs": []
        },
        "db:generate": {
            "cache": false
        },
        "build": {
            "dependsOn": ["^build", "^db:generate"],
            "outputs": ["dist/**"]
        },
        "lint:check":{
            "cache": false
        },
        "lint:fix":{},
        "lint:format":{
            "cache": false
        },
        "test": {
            "cache": false
        },
        "db-seed": {
            "cache": false
        },
        "db-reset":{
            "cache": false
        }
    }
}